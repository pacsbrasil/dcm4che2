<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.GPSPSRequest" access="FIELD">
    <table name="gpsps_req" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value strategy="@generation-type@" generator="seq"/>
        <sequence-generator name="seq" sequence-name="gpsps_req_pk_seq" />
      </id>
      <basic name="requestedProcedureID">
        <column name="req_proc_id" />
      </basic>
      <basic name="accessionNumber">
        <column name="accession_no" />
      </basic>
      <many-to-one name="gpsps" fetch="LAZY">
        <join-column name="gpsps_fk" />
      </many-to-one>
    </attributes>
  </entity>
</entity-mappings>
