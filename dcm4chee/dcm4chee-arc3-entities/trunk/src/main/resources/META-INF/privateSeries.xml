<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.PrivateSeries" access="FIELD">
    <table name="priv_series" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value strategy="@generation-type@" generator="seq"/>
        <sequence-generator name="seq" sequence-name="priv_series_pk_seq" />
      </id>
      <basic name="privateType">
        <column name="priv_type" />
      </basic>
      <basic name="seriesInstanceUID">
        <column name="series_iuid" nullable="false" />
      </basic>
      <basic name="sourceAET">
        <column name="src_aet" />
      </basic>
      <basic name="encodedAttributes">
        <column name="series_attrs" column-definition="@blob@" />
      </basic>
      <many-to-one name="study" fetch="LAZY">
        <join-column name="study_fk" />
      </many-to-one>
      <one-to-many name="instances" mapped-by="series" fetch="LAZY"/>
    </attributes>
  </entity>
</entity-mappings>
