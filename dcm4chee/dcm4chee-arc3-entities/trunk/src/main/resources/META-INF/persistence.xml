    <mapping-file>META-INF/privateStudy.xml</mapping-file>
    <mapping-file>META-INF/requestAttributes.xml</mapping-file>
    <mapping-file>META-INF/series.xml</mapping-file>
    <mapping-file>META-INF/study.xml</mapping-file>
    <mapping-file>META-INF/studyOnFileSystem.xml</mapping-file>
    <mapping-file>META-INF/studyPermission.xml</mapping-file>
    <mapping-file>META-INF/verifyingObserver.xml</mapping-file>
    <properties>
      <property name="hibernate.dialect" value="@dialect@" />
      <property name="hibernate.hbm2ddl.auto" value="@hibernate.hbm2ddl.auto@" />
      <property name="hibernate.jdbc.use_get_generated_keys"
        value="@use_get_generated_keys@" />
      <property name="hibernate.show_sql" value="@hibernate.show_sql@" />
    </properties>
  </persistence-unit>
</persistence>