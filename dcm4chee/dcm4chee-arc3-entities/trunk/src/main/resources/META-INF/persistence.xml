    <mapping-file>META-INF/orm.xml</mapping-file>
    <mapping-file>META-INF/ae.xml</mapping-file>
    <mapping-file>META-INF/code.xml</mapping-file>
    <mapping-file>META-INF/device.xml</mapping-file>
    <mapping-file>META-INF/file.xml</mapping-file>
    <mapping-file>META-INF/fileSystem.xml</mapping-file>
    <mapping-file>META-INF/gppps.xml</mapping-file>
    <mapping-file>META-INF/gpsps.xml</mapping-file>
    <mapping-file>META-INF/gpspsPerformer.xml</mapping-file>
    <mapping-file>META-INF/gpspsRequest.xml</mapping-file>
    <mapping-file>META-INF/hangingProtocol.xml</mapping-file>
    <mapping-file>META-INF/hpDefinition.xml</mapping-file>
    <mapping-file>META-INF/instance.xml</mapping-file>
    <mapping-file>META-INF/media.xml</mapping-file>
    <mapping-file>META-INF/mpps.xml</mapping-file>
