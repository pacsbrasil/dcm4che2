<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id$ -->
<server>
  <classpath codebase="lib" archives="dcm4jboss.jar"/>

  <mbean code="org.jboss.mq.server.jmx.Queue"
	 name="dcm4jboss.archive:service=Queue,name=MPPSScu">
     <depends optional-attribute-name="DestinationManager">
     	jboss.mq:service=DestinationManager
     </depends>
  </mbean>  

  <mbean code="org.dcm4chex.archive.dcm.mppsscu.MPPSScuService" 
	     name="dcm4jboss.archive:service=MPPSScu"
	     xmbean-dd="xmdesc/dcm4jboss-mppsscu-xmbean.xml">
    <depends>
      dcm4jboss.archive:service=MPPSScp
    </depends>
    <depends>
      dcm4jboss.archive:service=Queue,name=MPPSScu
    </depends>
    <depends>
      jboss.mq:service=InvocationLayer,type=JVM
    </depends>
  </mbean>
</server>

