<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id$ -->
<server>
  <classpath codebase="lib" archives="dcm4jboss.jar"/>
  
  <mbean code="org.jboss.mq.server.jmx.Queue"
	 name="dcm4jboss.archive:service=Queue,name=MoveScu">
     <depends optional-attribute-name="DestinationManager">
     	jboss.mq:service=DestinationManager
     </depends>
  </mbean>
   
  <mbean code="org.dcm4chex.archive.dcm.movescu.MoveScuService" 
	     name="dcm4jboss.archive:service=MoveScu"
	     xmbean-dd="xmdesc/dcm4jboss-movescu-xmbean.xml">
    <depends>dcm4jboss.archive:service=QueryRetrieveScp</depends>
    <depends>dcm4jboss.archive:service=TLSConfig</depends>
    <depends>dcm4jboss.archive:service=Queue,name=MoveScu</depends>
    <depends>jboss.mq:service=InvocationLayer,type=JVM</depends>
  </mbean>
</server>

