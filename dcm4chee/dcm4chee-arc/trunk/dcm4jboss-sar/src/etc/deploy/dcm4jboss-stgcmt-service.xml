<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id$ -->
<server>
    <classpath codebase="lib" archives="dcm4jboss.jar"/>

  <mbean code="org.jboss.mq.server.jmx.Queue"
	 name="dcm4jboss.archive:service=Queue,name=StgCmtScuScp">
     <depends optional-attribute-name="DestinationManager">
     	jboss.mq:service=DestinationManager
     </depends>
  </mbean>

    <mbean code="org.dcm4chex.archive.dcm.stgcmt.StgCmtScuScpService"
        name="dcm4jboss.archive:service=StgCmtScuScp" xmbean-dd="xmdesc/dcm4jboss-stgcmt-xmbean.xml">
        <depends>dcm4jboss.archive:service=AE</depends>
        <depends>dcm4jboss.archive:service=DcmServer</depends>
        <depends>dcm4jboss.archive:service=FileSystemMgt</depends>
        <depends>dcm4jboss.archive:service=Queue,name=StgCmtScuScp</depends>
        <depends>jboss.mq:service=InvocationLayer,type=JVM</depends>
    </mbean>
</server>
