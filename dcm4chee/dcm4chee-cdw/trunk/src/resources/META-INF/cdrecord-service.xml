<?xml version='1.0' encoding='UTF-8' ?>

<!-- $Id$ -->

<!DOCTYPE server PUBLIC
   "-//JBoss//DTD MBean Service 3.2//EN"
   "http://www.jboss.org/j2ee/dtd/jboss-service_3_2.dtd"
>

<server>

  <mbean code="org.jboss.mq.server.jmx.Queue"
	 name="dcm4jboss.dcmcdw:service=Queue,name=CDRecord">
     <depends optional-attribute-name="DestinationManager">
     	jboss.mq:service=DestinationManager
     </depends>
  </mbean>  

   <mbean code="org.dcm4chex.cdw.cdrecord.CDRecordService"
      name="dcm4jboss.dcmcdw:service=MediaWriter,name=CDRecord"
      xmbean-dd="xmdesc/dcmcdw-cdrecord-xmbean.xml">
      <depends>
      	dcm4jboss.dcmcdw:service=Queue,name=CDRecord
      </depends>
      <depends>
        jboss.mq:service=InvocationLayer,type=JVM
      </depends>
      <depends>
        dcm4jboss.dcmcdw:service=LabelPrint
      </depends>
   </mbean>
</server>
