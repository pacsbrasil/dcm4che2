<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.Media">
    <table name="media" />
    <pre-persist method-name="onPrePersist" />
    <pre-update method-name="onPreUpdate" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value />
      </id>
      <basic name="createdTime">
        <column name="created_time" />
      </basic>
      <basic name="updatedTime">
        <column name="updatedTime" />
      </basic>
      <basic name="mediaCreationRequestInstanceUID">
        <column name="media_rq_iuid" />
      </basic>
      <basic name="mediaStatus">
        <column name="media_status" />
      </basic>
      <basic name="mediaStatusInfo">
        <column name="media_status_info" />
      </basic>
      <basic name="mediaUsage">
        <column name="media_usage" />
      </basic>
      <basic name="fileSetID">
        <column name="fileset_id" />
      </basic>
      <basic name="fileSetUID">
        <column name="fileset_iuid" />
      </basic>
      <one-to-many name="instances" mapped-by="media" />
    </attributes>
  </entity>
</entity-mappings>
