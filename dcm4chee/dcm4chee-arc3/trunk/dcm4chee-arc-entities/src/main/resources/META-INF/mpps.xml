<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.MPPS">
    <table name="mpps" />
    <pre-persist method-name="onPrePersist" />
    <pre-update method-name="onPreUpdate" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value />
      </id>
      <basic name="createdTime">
        <column name="created_time" />
      </basic>
      <basic name="updatedTime">
        <column name="updatedTime" />
      </basic>
      <basic name="sopInstanceUID">
        <column name="mpps_iuid" unique="true" nullable="false" />
      </basic>
      <basic name="startDateTime">
        <column name="pps_start" />
      </basic>
      <basic name="performedStationAET">
        <column name="station_aet" />
      </basic>
      <basic name="modality">
        <column name="modality" />
      </basic>
      <basic name="accessionNumber">
        <column name="accession_no" />
      </basic>
      <basic name="status">
        <column name="mpps_status" />
      </basic>
      <basic name="encodedAttributes">
        <column name="mpps_attrs" />
      </basic>
      <many-to-one name="discontinuationReasonCode">
        <join-column name="drcode_fk" />
      </many-to-one>
      <many-to-one name="patient">
        <join-column name="patient_fk" />
      </many-to-one>
      <one-to-many name="series" mapped-by="modalityPerformedProcedureStep" />
    </attributes>
  </entity>
</entity-mappings>
