<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.FileSystem" access="FIELD">
    <table name="filesystem" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value strategy="@generation-type@" generator="seq"/>
        <sequence-generator name="seq" sequence-name="filesystem_pk_seq" />
      </id>
      <basic name="directoryPath">
        <column name="dirpath" nullable="false" unique="true" />
      </basic>
      <basic name="groupID">
        <column name="fs_group_id" />
      </basic>
      <basic name="retrieveAET">
        <column name="retrieve_aet" />
      </basic>
      <basic name="availability">
        <column name="availability" />
      </basic>
      <basic name="status">
        <column name="fs_status" />
      </basic>
      <basic name="userInfo">
        <column name="user_info" />
      </basic>
      <one-to-one name="nextFileSystem" fetch="LAZY">
        <join-column name="next_fk" />
      </one-to-one>
    </attributes>
  </entity>
</entity-mappings>
