<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.PrivateInstance">
    <table name="priv_instance" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value />
      </id>
      <basic name="privateType">
        <column name="priv_type" />
      </basic>
      <basic name="sopInstanceUID">
        <column name="sop_iuid" nullable="false" />
      </basic>
      <basic name="encodedAttributes">
        <column name="inst_attrs" />
      </basic>
      <many-to-one name="series">
        <join-column name="series_fk" />
      </many-to-one>
      <one-to-many name="files" mapped-by="instance" />
    </attributes>
  </entity>
</entity-mappings>
