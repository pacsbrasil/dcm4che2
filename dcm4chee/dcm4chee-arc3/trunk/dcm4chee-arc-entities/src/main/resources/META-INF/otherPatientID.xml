<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.OtherPatientID">
    <table name="other_pid" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value />
      </id>
      <basic name="patientID">
        <column name="pat_id" nullable="false" />
      </basic>
      <basic name="issuerOfPatientID">
        <column name="pat_id_issuer" nullable="false" />
      </basic>
      <many-to-many name="patients" mapped-by="otherPatientIDs" />
    </attributes>
  </entity>
</entity-mappings>
