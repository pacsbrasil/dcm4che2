<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.AE" access="FIELD">
    <table name="ae" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value strategy="@generation-type@" generator="seq"/>
        <sequence-generator name="seq" sequence-name="ae_pk_seq" />
      </id>
      <basic name="title">
        <column name="aet" nullable="false" />
      </basic>
      <basic name="hostName">
        <column name="hostname" nullable="false" />
      </basic>
      <basic name="port">
        <column name="port" nullable="false" />
      </basic>
      <basic name="cipherSuites">
        <column name="cipher_suites" />
      </basic>
      <basic name="issuerOfPatientID">
        <column name="pat_id_issuer" />
      </basic>
      <basic name="userID">
        <column name="user_id" />
      </basic>
      <basic name="password">
        <column name="passwd" />
      </basic>
      <basic name="fileSystemGroupID">
        <column name="fs_group_id" />
      </basic>
      <basic name="description">
        <column name="ae_desc" />
      </basic>
      <basic name="wadoURL">
        <column name="wado_url" />
      </basic>
    </attributes>
  </entity>
</entity-mappings>
