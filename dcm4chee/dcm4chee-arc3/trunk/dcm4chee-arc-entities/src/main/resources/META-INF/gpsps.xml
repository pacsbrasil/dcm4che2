<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.GPSPS">
    <table name="gpsps" />
    <pre-persist method-name="onPrePersist" />
    <pre-update method-name="onPreUpdate" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value />
      </id>
      <basic name="createdTime">
        <column name="created_time" />
      </basic>
      <basic name="updatedTime">
        <column name="updated_time" />
      </basic>
      <basic name="sopInstanceUID">
        <column name="gpsps_iuid" />
      </basic>
      <basic name="transactionUID">
        <column name="gpsps_tuid" />
      </basic>
      <basic name="startDateTime">
        <column name="start_datetime" />
      </basic>
      <basic name="expectedCompletionDateTime">
        <column name="end_datetime" />
      </basic>
      <basic name="status">
        <column name="gpsps_status" />
      </basic>
      <basic name="priority">
        <column name="gpsps_prior" />
      </basic>
      <basic name="inputAvailability">
        <column name="in_availability" />
      </basic>
      <basic name="encodedAttributes">
        <column name="item_attrs" />
      </basic>
      <many-to-one name="patient">
        <join-column name="patient_fk" />
      </many-to-one>
      <many-to-one name="scheduledWorkItemCode">
        <join-column name="code_fk" />
      </many-to-one>
      <one-to-many name="referencedRequests" mapped-by="gpsps" />
      <one-to-many name="scheduledHumanPerformers"
        mapped-by="gpsps" />
      <many-to-many name="scheduledProcessingApplicationsCodes">
        <join-table name="rel_gpsps_appcode">
          <join-column name="appcode_fk" />
          <inverse-join-column name="gpsps_fk" />
        </join-table>
      </many-to-many>
      <many-to-many name="scheduledStationNameCodes">
        <join-table name="rel_gpsps_devname">
          <join-column name="devname_fk" />
          <inverse-join-column name="gpsps_fk" />
        </join-table>
      </many-to-many>
      <many-to-many name="scheduledStationClassCodes">
        <join-table name="rel_gpsps_devclass">
          <join-column name="devclass_fk" />
          <inverse-join-column name="gpsps_fk" />
        </join-table>
      </many-to-many>
      <many-to-many name="scheduledStationGeographicLocationCodes">
        <join-table name="rel_gpsps_devloc">
          <join-column name="devloc_fk" />
          <inverse-join-column name="gpsps_fk" />
        </join-table>
      </many-to-many>
      <many-to-many name="performedProcedureSteps"
        mapped-by="scheduledProcedureSteps" />
    </attributes>
  </entity>
</entity-mappings>
