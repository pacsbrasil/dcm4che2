<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.MWLItem" access="FIELD">
    <table name="mwl_item" />
    <pre-persist method-name="onPrePersist" />
    <pre-update method-name="onPreUpdate" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value strategy="@generation-type@" generator="seq"/>
        <sequence-generator name="seq" sequence-name="mwl_item_pk_seq" />
      </id>
      <basic name="createdTime">
        <column name="created_time" />
      </basic>
      <basic name="updatedTime">
        <column name="updated_time" />
      </basic>
      <basic name="scheduledProcedureStepID">
        <column name="sps_id" />
      </basic>
      <basic name="requestedProcedureID">
        <column name="req_proc_id" />
      </basic>
      <basic name="studyInstanceUID">
        <column name="study_iuid" />
      </basic>
      <basic name="accessionNumber">
        <column name="accession_no" />
      </basic>
      <basic name="modality">
        <column name="modality" />
      </basic>
      <basic name="scheduledStationAET">
        <column name="station_aet" />
      </basic>
      <basic name="scheduledStationName">
        <column name="station_name" />
      </basic>
      <basic name="startDateTime">
        <column name="start_datetime" />
      </basic>
      <basic name="scheduledPerformingPhysicianName">
        <column name="perf_physician" column-definition="@nvarchar2@" />
      </basic>
      <basic name="scheduledPerformingPhysicianIdeographicName">
        <column name="perf_phys_i_name" column-definition="@nvarchar2@" />
      </basic>
      <basic name="scheduledPerformingPhysicianPhoneticName">
        <column name="perf_phys_p_name" column-definition="@nvarchar2@" />
      </basic>
      <basic name="status">
        <column name="sps_status" />
      </basic>
      <basic name="encodedAttributes">
        <column name="item_attrs" column-definition="@blob@" />
      </basic>
      <many-to-one name="patient">
        <join-column name="patient_fk" nullable="false" />
      </many-to-one>
    </attributes>
  </entity>
</entity-mappings>
