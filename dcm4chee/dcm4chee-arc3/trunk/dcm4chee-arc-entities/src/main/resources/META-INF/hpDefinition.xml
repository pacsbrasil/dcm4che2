<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.HPDefinition">
    <table name="hpdef" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value />
      </id>
      <basic name="modality">
        <column name="modality" />
      </basic>
      <basic name="laterality">
        <column name="laterality" />
      </basic>
      <many-to-one name="hangingProtocol">
        <join-column name="hp_fk" />
      </many-to-one>
      <many-to-many name="anatomicRegionCodes">
        <join-table name="rel_hpdef_region">
          <join-column name="hpdef_fk" />
          <inverse-join-column name="region_fk" />
        </join-table>
      </many-to-many>
      <many-to-many name="procedureCodes">
        <join-table name="rel_hpdef_proc">
          <join-column name="hpdef_fk" />
          <inverse-join-column name="proc_fk" />
        </join-table>
      </many-to-many>
      <many-to-many name="reasonforRequestedCodes">
        <join-table name="rel_hpdef_reason">
          <join-column name="hpdef_fk" />
          <inverse-join-column name="reason_fk" />
        </join-table>
      </many-to-many>
    </attributes>
  </entity>
</entity-mappings>
