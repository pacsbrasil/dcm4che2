<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  <entity class="org.dcm4chee.archive.entity.PrivateStudy">
    <table name="priv_study" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value />
      </id>
      <basic name="privateType">
        <column name="priv_type" />
      </basic>
      <basic name="studyInstanceUID">
      	<column name="study_iuid" nullable="false"/>
      </basic>
      <basic name="accessionNumber">
      	<column name="accession_no"/>
      </basic>
      <basic name="encodedAttributes">
      	<column name="study_attrs"/>
      </basic>
      <many-to-one name="patient">
      	<join-column name="patient_fk"/>
      </many-to-one>
      <one-to-many name="series" mapped-by="study"/>
    </attributes>
  </entity>
</entity-mappings>
